[user]
	name = yan.zhu
	email = yan.zhush@ele.me
[core]
	editor = vim
	excludesfile = /Users/eleme/.gitignore_global
[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	co = checkout
	br = branch
	ci = commit
	st = status
	unstage = reset HEAD --
	uncommit = reset --soft HEAD~
	last = log -1 HEAD
	pro = pull --rebase origin
	pru = pull --rebase upstream
	getsub = submodule update --init --recursive
[pull]
	default = current
[push]
	default = current
